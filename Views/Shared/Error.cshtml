@{
    ViewData["Title"] = "404";
}

<!-- error section -->
<div class="full-height-section error-section">
	<div class="full-height-tablecell">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2 text-center">
					<div class="error-text">
						<i class="far fa-sad-cry"></i>
						<h1>Lỗi không tìm thấy trang.</h1>
						<p>Có vẻ như các trang mà bạn đang cố gắng tiếp cận không tồn tại. Xin lỗi về sự bất tiện này</p>
						<a asp-action="index" asp-controller="home" class="boxed-btn">Quay lại trang chủ</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- end error section -->
